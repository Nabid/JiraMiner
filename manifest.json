{
	"manifest_version": 2,
	
	"name": "JIRA Email Miner",
	"description": "Mines texts from daily plan in JIRA and builds, formats email to be sent to client",
	"version": "0.1",
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/",
		"https://planetdds.atlassian.net/browse/SP*",
		"http://192.168.1.62/*"
	]
}